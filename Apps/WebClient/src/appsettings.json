{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "ForwardProxies": {
        "Enabled": "true",
        "KnownProxies": []
    },
    "SwaggerSettings": {
        "RoutePrefix": "swagger",
        "Info": {
            "Title": "Health Gateway WebClient Services documentation",
            "Description": "Provides API documentation for Health Gateway WebClient Services.",
            "License": {
                "Name": "Apache 2.0",
                "Url": "https://www.apache.org/licenses/LICENSE-2.0.html"
            }
        }
    },
    "OpenIdConnect": {
        "ResponseType": "code",
        "Scope": "openid profile"
    },
    "IdentityProviders": [
        {
            "id": "BCSC",
            "Name": "BC Services Card",
            "Icon": "address-card",
            "Hint": "bcsc",
            "disabled": "false"
        }
    ],
    "WebClient": {
        "RedirectToWWW": "true",
        "LogLevel": "Debug",
        "TimeOuts": {
            "Idle": "900000",
            "LogoutRedirect": "20000",
            "ResendSMS": "2"
        },
        "ExternalURLs": {
            "HealthLinkImmunizationSchedule": "https://www.healthlinkbc.ca/tools-videos/bc-immunization-schedules",
            "HealthLinkVaccineSearch": "https://www.healthlinkbc.ca/search/"
        },
        "Modules": {
            "Immunization": "false",
            "Medication": "true",
            "Laboratory": "false",
            "Encounter": "false",
            "Note": "false",
            "Comment": "false",
            "CovidLabResults": "false"
        },
        "HoursForDeletion": 720
    },
    "AvailabilityFilter": {
        "DumpHeaders": "false",
        "DumpHeaders.Index": "false"
    },
    "PatientService": {
        "ClientRegistry": {
            "ServiceUrl": "https://wembley.hlth.gov.bc.ca/HI3/HCIM.HIALServices.Portal/QUPA_AR101102.asmx",
            "ClientCertificate": {
                "Path": "/usr/local/HealthGateway/HGWAY_HI1.pfx"
            }
        },
        "CacheTTL": 90
    },
    "OpenTelemetry": {
        "Enabled": false,
        "Sources": [ "PatientService", "ClientRegistriesDelegate" ],
        "ServiceName": "WebClient",
        "ConsoleEnabled": false,
        "ZipkinEnabled": false,
        "ZipkinUri": ""
    }
}
