{
    "Logging": {
        "LogLevel": {
            "HealthGateway": "Trace"
        }
    },
    "OpenIdConnect": {
        "Authority": "https://dev.oidc.gov.bc.ca/auth/realms/ff09qn3f",
        "Callbacks": {
            "Logon": "https://dev.healthgateway.gov.bc.ca/loginCallback",
            "Logout": "https://dev.healthgateway.gov.bc.ca/logoutComplete"
        }
    },
    "IdentityProviders": [
        {
            "id": "BCSC",
            "Name": "BC Services Card",
            "Icon": "address-card",
            "Hint": "bcsc",
            "disabled": false
        },
        {
            "id": "IDIR",
            "Name": "BC Government IDIR",
            "Icon": "user",
            "Hint": "idir",
            "disabled": false
        },
        {
            "id": "KeyCloak",
            "Name": "KeyCloak",
            "Icon": "user-secret",
            "Hint": "",
            "disabled": false
        }
    ],
    "WebClient": {
        "RedirectToWWW": false,
        "LogLevel": "Debug",
        "TimeOuts": {
            "ResendSMS": "1"
        },
        "ExternalURLs": {
            "CredentialVerification": "https://health-gateway-verifier-dev.apps.silver.devops.gov.bc.ca"
        },
        "Modules": {
            "DependentImmunizationTab": true,
            "WalletExport": true
        }
    },
    "ServiceEndpoints": {
        "Encounter": "https://hg-dev.api.gov.bc.ca/encounter/",
        "GatewayApi": "https://hg-dev.api.gov.bc.ca/gatewayapi/",
        "Immunization": "https://hg-dev.api.gov.bc.ca/immunization/",
        "Laboratory": "https://hg-dev.api.gov.bc.ca/laboratory/",
        "Medication": "https://hg-dev.api.gov.bc.ca/medication/",
        "Patient": "https://hg-dev.api.gov.bc.ca/patient/"
    },
    "PatientService": {
        "ClientRegistry": {
            "ServiceUrl": "https://hiat2.hcim.ehealth.gov.bc.ca/HCIM.HIALServices.Portal/QUPA_AR101102.asmx",
            "ClientCertificate": {
                "Path": "/usr/local/HealthGateway/HGWAY_HI1.pfx"
            }
        },
        "CacheTTL": 90
    },
    "ContentSecurityPolicy": {
        "ConnectSource": "'self' file: data: blob: filesystem: https://spt.apps.gov.bc.ca/com.snowplowanalytics.snowplow/tp2 https://dev.oidc.gov.bc.ca/ https://hg-dev.api.gov.bc.ca/",
        "FrameSource": "'self' https://dev.oidc.gov.bc.ca/",
        "FrameAncestors": "'self' https://dev.oidc.gov.bc.ca/"
    },
    "OpenTelemetry": {
        "Enabled": true,
        "ConsoleEnabled": true
    },
    "MobileConfiguration": {
        "BaseUrl": "https://dev.healthgateway.gov.bc.ca/"
    }
}
