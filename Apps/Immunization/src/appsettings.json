{
    "Logging": {
        "LogLevel": {
            "HealthGateway.Common.Instrumentation": "Warning",
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ForwardProxies": {
        "Enabled": "true",
        "BasePath": "/api/immunizationservice",
        "KnownProxies": []
    },
    "AllowedHosts": "*",
    "OpenIdConnect": {
        "Authority": "https://*",
        "Audience": "healthgateway"
    },
    "SwaggerSettings": {
        "RoutePrefix": "swagger",
        "Info": {
            "Title": "Health Gateway Immunization Services documentation",
            "Description": "Provides API documentation for Health Gateway Immunization Service.",
            "License": {
                "Name": "Apache 2.0",
                "Url": "https://www.apache.org/licenses/LICENSE-2.0.html"
            }
        }
    },
    "Immunization": {
        "EndPoint": "https://phsahealthgatewayapi.azurewebsites.net/api/v1/Immunizations",
        "FetchSize": "25"
    },
    "OpenTelemetry": {
        "Enabled": false,
        "Sources": [ "RestImmunizationDelegate", "PatientService", "ClientRegistriesDelegate" ],
        "ServiceName": "ImmunizationService",
        "ConsoleEnabled": false,
        "ZipkinEnabled": false,
        "ZipkinUri": ""
    }
}
