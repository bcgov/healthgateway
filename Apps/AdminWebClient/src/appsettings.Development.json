{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 10,
      "MaxConcurrentUpgradedConnections": 10
    }
  },
  "AllowedHosts": "*",
  "AddCors": {
    "WithOrigins": "http://localhost:5010"
  }
}