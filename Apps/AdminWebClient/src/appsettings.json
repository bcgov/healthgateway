{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning"
        }
    },
    "OpenIdConnect": {
        "Authority": "https://oidc.gov.bc.ca/auth/realms/ff09qn3f",
        "ClientId": "healthgateway-admin",
        "Audience": "healthgateway-admin",
        "SaveTokens": "true",
        "GetClaimsFromUserInfoEndpoint": "true",
        "SignedOutRedirectUri": "/signoff",
        "ResponseType": "code",
        "Scope": "openid profile email offline_access"
    },
    "EnabledRoles": ["AdminUser", "AdminReviewer"],
    "SwaggerSettings": {
        "RoutePrefix": "swagger",
        "Info": {
            "Title": "Health Gateway Admin WebClient Services documentation",
            "Description": "Provides API documentation for Health Gateway Admin WebClient Services.",
            "License": {
                "Name": "Apache 2.0",
                "Url": "https://www.apache.org/licenses/LICENSE-2.0.html"
            }
        }
    },
    "KeyCloak": {
        "IDPHintKey": "kc_idp_hint",
        "IDPHint": "idir"
    },
    "Admin": {
        "LogLevel": "Debug",
        "TimeOuts": {
            "Idle": "900000",
            "LogoutRedirect": "20000"
        },
        "UnixTimeZoneId": "America/Vancouver",
        "WindowsTimeZoneId": "Pacific Standard Time"
    },
    "ServiceEndpoints": {
        "CsvExportBaseUri": "v1/api/CsvExport",
        "JobScheduler": "https://www.healthgateway.gov.bc.ca/admin/jobscheduler"
    },
    "ForwardProxies": {
        "Enabled": "true",
        "BasePath": "/admin",
        "IPs": []
    },
    "ContentSecurityPolicy": {
        "connect-src": "https://oidc.gov.bc.ca/",
        "frame-src": "https://oidc.gov.bc.ca/",
        "script-src": "https://www2.gov.bc.ca/StaticWebResources/static/sp/sp-2-14-0.js 'sha256-q+lOQm0t+vqQq4IdjwI4OwRI9fKfomdxduL1IJYjkA4='",
        "style-src": "https://fonts.googleapis.com/ https://cdn.jsdelivr.net/ https://use.fontawesome.com/",
        "font-src": "https://fonts.gstatic.com/ https://cdn.jsdelivr.net/ https://use.fontawesome.com/"
    },
    "PatientService": {
        "ClientRegistry": {
            "ServiceUrl": "https://hcim.ehealth.gov.bc.ca/HCIM.HIALServices.Portal/QUPA_AR101102.asmx",
            "ClientCertificate": {
                "Path": "/usr/local/HealthGateway/HGWAY_HP1.pfx"
            }
        },
        "CacheTTL": 90
    },
    "DocumentStorage": {
        "SftpHostname": "ftpsvcs.hlth.gov.bc.ca",
        "SftpPort": 22,
        "SftpUsername": "***",
        "SftpFolderPath": "ToMaximus",
        "PrivateKeyPath": "/usr/local/sftp/HG_SFTP_2021-Prod.pem",
        "PrivateKeyPassphrase": "***"
    },
    "Immunization": {
        "EndPoint": "https://phsahealthgatewayapi-prod.azurewebsites.net/api/v1/Support/Immunizations",
        "FetchSize": "25"
    },
    "CDOGS": {
        "ServiceName": "HGCDOGS_SERVICE",
        "ServiceHostSuffix": "_HOST",
        "ServicePortSuffix": "_PORT",
        "DynamicServiceLookup": "true",
        "BaseEndpoint": "http://${serviceHost}:${servicePort}/"
    }
}
