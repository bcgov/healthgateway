{
    "env": {
        "cypress/globals": true,
        "es2021": true
    },
    "plugins": ["cypress", "chai-friendly"],
    "extends": [
        "plugin:chai-friendly/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prefer-spread": "off",
        "prefer-rest-params": "off"
    }
}
