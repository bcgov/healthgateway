{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning"
        }
    },
    "OpenIdConnect": {
        "Authority": "https://oidc.gov.bc.ca/auth/realms/ff09qn3f",
        "ClientId": "healthgateway",
        "Audience": "healthgateway"
    },
    "ForwardProxies": {
        "Enabled": "true",
        "BasePath": "/api/laboratoryservice",
        "KnownProxies": []
    },
    "AllowedHosts": "*",
    "SwaggerSettings": {
        "RoutePrefix": "api/laboratoryservice/swagger",
        "Info": {
            "Title": "Health Gateway Laboratory Services documentation",
            "Description": "Provides API documentation for Health Gateway Laboratory Service.",
            "License": {
                "Name": "Apache 2.0",
                "Url": "https://www.apache.org/licenses/LICENSE-2.0.html"
            }
        }
    },
    "Laboratory": {
        "EndPoint": "https://phsahealthgatewayapi.azurewebsites.net/api/v1/LabOrders",
        "FetchSize": "25"
    },
    "LaboratoryDelegate": "HealthGateway.Laboratory.Delegates.MockLaboratoryDelegate",
    "OpenTelemetry": {
        "Enabled": false,
        "Sources": [ "RestLaboratoryDelegate", "PatientService", "ClientRegistriesDelegate" ],
        "ServiceName": "LaboratoryService",
        "ConsoleEnabled": false,
        "ZipkinEnabled": false,
        "ZipkinUri": ""
    }
}
